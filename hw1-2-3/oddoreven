#include <iostream>

using namespace std;

int main(){

 int userinput;

    cout << "Enter an integer: " << endl;
    cin  >> userinput;

    if (userinput % 2 == 0) {
        cout << userinput << " is an even number!" << endl;
    }
    else {
        cout << userinput << " is an odd number!" << endl;  
    }







    return 0;
}
